{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "api/blob/*.js", "use": "@vercel/node" },
    { "src": "api/blob/*.mjs", "use": "@vercel/edge" }
  ],
  "routes": [
    { "src": "/api/blob/upload", "dest": "/api/blob/upload.mjs" },
    { "src": "/api/blob/(.*)", "dest": "/api/blob/$1" },
    { "src": "/static/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
